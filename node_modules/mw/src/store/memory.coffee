module.exports = ->
    sessions = {}
    getByIdAndSecret: (id, secret, next) ->
        result = {}
        result = JSON.parse sessions[id].session if sessions[id]?.secret is secret
        next null, result
    setByIdAndSecret: (id, secret, session, cb) ->
        sessions[id] = {secret: secret, session: JSON.stringify session}
        cb()

    deleteOlderThen: (date, cb) -> cb null
